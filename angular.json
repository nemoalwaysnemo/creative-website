{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tbwa-knowledge-website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "project",
      "sourceRoot": "project/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "project/dist/LibraryWebUI",
            "index": "project/src/index.html",
            "rebaseRootRelativeCssUrls": true,
            "main": "project/src/main.ts",
            "polyfills": "project/src/polyfills.ts",
            "tsConfig": "project/tsconfig.app.json",
            "aot": true,
            "assets": [
              "project/src/assets",
              "project/src/favicon.ico",
              "project/src/favicon.png"
            ],
            "styles": [
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/typeface-exo/index.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "project/src/app/core/nebular/icons/scss/nebular-icons.scss",
              "project/src/app/theme/fonts/custom-fonts.scss",
              "project/src/app/theme/styles/styles.scss"
            ],
            "scripts": [
              "node_modules/pace-js/pace.min.js",
              "node_modules/hammerjs/hammer.min.js"
            ],
            "allowedCommonJsDependencies": [
              "rxjs",
              "lodash",
              "angular2-text-mask"
            ]
          },
          "configurations": {
            "aws-prod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "deployUrl": "/nuxeo/LibraryWebUI/",
              "baseHref": "/nuxeo/site/LibraryWebUI/",
              "fileReplacements": [{
                "replace": "project/src/environments/environment.ts",
                "with": "project/src/environments/environment.aws-prod.ts"
              }]
            },
            "aws-stg": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "deployUrl": "/nuxeo/LibraryWebUI/",
              "baseHref": "/nuxeo/site/LibraryWebUI/",
              "fileReplacements": [{
                "replace": "project/src/environments/environment.ts",
                "with": "project/src/environments/environment.aws-stg.ts"
              }]
            },
            "aws-dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle"
                }
              ],
              "deployUrl": "/nuxeo/LibraryWebUI/",
              "baseHref": "/nuxeo/site/LibraryWebUI/",
              "fileReplacements": [{
                "replace": "project/src/environments/environment.ts",
                "with": "project/src/environments/environment.aws-dev.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tbwa-knowledge-website:build",
            "sslKey": "cert/ssl.local-key.pem",
            "sslCert": "cert/ssl.local.pem",
            "ssl": true
          },
          "configurations": {
            "production": {
              "browserTarget": "tbwa-knowledge-website:build:aws-prod"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tbwa-knowledge-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "project/src/test.ts",
            "polyfills": "project/src/polyfills.ts",
            "tsConfig": "project/tsconfig.spec.json",
            "karmaConfig": "project/karma.conf.js",
            "assets": [
              "project/src/favicon.ico",
              "project/src/assets"
            ],
            "styles": [
              "project/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "project/tsconfig.app.json",
              "project/tsconfig.spec.json",
              "project/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tbwa-knowledge-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tbwa-knowledge-website:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "tbwa-knowledge-website"
}
