{
  "manifest_version": 2,
  "name": "tbwa-knowledge-website",
  "short_name": "tbwa-knowledge",
  "version": "1.0.0",
  "description": "TBWA/ Knowledge",
  "permissions": [
    "contextMenus",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html?#/popup",
    "default_title": "TBWA/ Knowledge"
  },
  "content_scripts": [{
    "js": [
      "contentPage.js"
    ],
    "matches": [
      "<all_urls>"
    ]
  }],
  "background": {
    "scripts": [
      "backgroundPage.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
