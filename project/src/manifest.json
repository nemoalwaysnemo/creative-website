{
  "manifest_version": 2,
  "name": "tbwa-knowledge-website",
  "short_name": "tbwa-knowledge",
  "version": "1.0.0",
  "description": "TBWA/ Knowledge",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://*.okta.com/*",
    "*://*.factory.tools/*"
  ],
  "browser_action": {
    "default_popup": "index.html/#/p/knowledge/home",
    "default_title": "TBWA/ Knowledge"
  },
  "web_accessible_resources": [
    "images/*"
  ],
  "content_scripts": [{
    "js": [
      "contentPage.js"
    ],
    "matches": [
      "<all_urls>"
    ]
  }],
  "background": {
    "scripts": [
      "backgroundPage.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-vqHqWa3DPq/BfYYTA//1EXFm+JSPXl1Iy3L4IYpI858='; object-src 'self' https://*.factory.tools; "
}
