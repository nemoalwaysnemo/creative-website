    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lite-server": "^2.5.4",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "standard-version": "^7.0.0",
    "stylelint": "11.1.1",
    "ts-md5": "^1.2.4",
    "ts-node": "8.3.0",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
