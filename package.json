{
  "name": "tbwa-knowledge-website",
  "version": "4.0.1",
  "scripts": {
    "commit": "git-cz",
    "build:prod": "ng build --c=aws-prod",
    "build:stg": "ng build --c=aws-stg",
    "build:dev": "ng build --c=aws-dev",
    "build:rdev": "ng build --c=rails-dev",
    "build:test": "ng build --c=local-dev",
    "build:serve": "ng build --c=local-dev && yarn serve",
    "lint": "ng lint",
    "lint:fix": "ng lint tbwa-knowledge-website --fix",
    "lint:styles": "stylelint ./src/**/*.scss --fix",
    "lint:ci": "yarn lint:fix && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version",
    "postinstall": "ngcc",
    "serve": "lite-server --baseDir=./dist/LibraryWebUI"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.1",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.1",
    "@angular/compiler": "~10.1.1",
    "@angular/core": "~10.1.1",
    "@angular/forms": "~10.1.1",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.1",
    "@angular/platform-browser-dynamic": "~10.1.1",
    "@angular/router": "~10.1.1",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@ng-select/ng-select": "^5.0.1",
    "@videogular/ngx-videogular": "^2.0.1",
    "angular2-text-mask": "^9.0.0",
    "angular2-toaster": "^8.0.0",
    "bootstrap": "^4.3.1",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.11.0",
    "ng-click-outside": "^7.0.0",
    "ng2-completer": "^9.0.1",
    "ng5-slider": "^1.2.4",
    "ngx-clipboard": "^13.0.1",
    "ngx-device-detector": "^2.0.0",
    "ngx-drag-scroll": "^9.0.0-beta.4",
    "ngx-infinite-scroll": "^9.1.0",
    "ngx-permissions": "^8.0.0",
    "ngx-pipes": "^2.7.5",
    "ngx-scrollbar": "^7.3.0",
    "pace-js": "^1.0.2",
    "parse5": "^6.0.1",
    "postinstall": "^0.7.0",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.6.3",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "typeface-exo": "^0.0.61",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.1",
    "@angular/cli": "~10.1.1",
    "@angular/compiler-cli": "~10.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.7.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
