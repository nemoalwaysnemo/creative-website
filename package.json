{
  "name": "tbwa-creative-website",
  "version": "2.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+git@gitlab-ssh-eu.factory.tools:WTG-Dev/tbwa-creative-website.git"
  },
  "bugs": {
    "url": "https://gitlab-eu.factory.tools/WTG-Dev/tbwa-creative-website/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "build:prod": "ng build --c=aws-prod",
    "build:stg": "ng build --c=aws-stg",
    "build:dev": "ng build --c=aws-dev",
    "build:rdev": "ng build --c=rails-dev",
    "build:test": "ng build --c=local-dev",
    "build:serve": "ng build --c=local-dev && yarn serve",
    "lint": "ng lint",
    "lint:fix": "ng lint tbwa-creative-website --fix",
    "lint:styles": "stylelint ./src/**/*.scss --fix",
    "lint:ci": "yarn lint && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version",
    "serve": "lite-server --baseDir=./dist/LibraryWebUI"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
