{
  "name": "tbwa-knowledge-website",
  "version": "4.0.1",
  "scripts": {
    "commit": "git-cz",
    "build:prod": "yarn clean && ng build --c=aws-prod",
    "build:stg": "yarn clean && ng build --c=aws-stg",
    "build:dev": "yarn clean && ng build --c=aws-dev",
    "build:test": "yarn clean && ng build --c=local-dev",
    "build:serve": "yarn clean && ng build --c=local-dev && yarn serve",
    "build:chrome": "yarn clean && webpack --config chrome/webpack.config.js",
    "build:chrome-prod": "yarn clean && webpack -p --config chrome/webpack.config.prod.js",
    "lint": "ng lint",
    "lint:fix": "ng lint tbwa-knowledge-website --fix",
    "lint:styles": "stylelint ./project/src/**/*.scss --fix",
    "lint:ci": "yarn lint:fix && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version",
    "postinstall": "ngcc",
    "chrome": "yarn watch",
    "clean": "rimraf project/dist",
    "watch:project": "ng build --c=chrome-dev --watch --delete-output-path false",
    "watch:chrome": "webpack --config chrome/webpack.config.js -w",
    "watch": "yarn clean && npm-run-all --parallel watch:*",
    "pack": "cd project/dist && bestzip ../../extension-build.zip *",
    "serve": "lite-server --baseDir=./project/dist/LibraryWebUI"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~10.2.3",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.2.3",
    "@angular/compiler": "~10.2.3",
    "@angular/core": "~10.2.3",
    "@angular/forms": "~10.2.3",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.3",
    "@angular/platform-browser-dynamic": "~10.2.3",
    "@angular/router": "~10.2.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@ng-select/ng-select": "^5.0.8",
    "@videogular/ngx-videogular": "^2.1.0",
    "angular2-text-mask": "^9.0.0",
    "angular2-toaster": "^8.0.0",
    "bootstrap": "^4.3.1",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^3.7.0",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.11.0",
    "ng-click-outside": "^7.0.1",
    "ng2-completer": "^9.0.1",
    "ngx-clipboard": "^14.0.1",
    "ngx-device-detector": "^2.0.0",
    "ngx-drag-scroll": "^9.0.0-beta.4",
    "ngx-infinite-scroll": "^10.0.0",
    "ngx-permissions": "^8.1.1",
    "ngx-pipes": "^2.7.5",
    "ngx-scrollbar": "^7.3.1",
    "pace-js": "^1.0.2",
    "parse5": "^6.0.1",
    "postinstall": "^0.7.0",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.6.3",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.3",
    "typeface-exo": "^0.0.61",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.3",
    "@types/chrome": "^0.0.126",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "awesome-typescript-loader": "^5.2.1",
    "bestzip": "^2.1.7",
    "codelyzer": "^6.0.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.2",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "typescript-tslint-plugin": "^0.5.5",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0"
  }
}
