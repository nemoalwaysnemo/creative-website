{
  "name": "tbwa-creative-website",
  "version": "2.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+git@gitlab-ssh-eu.factory.tools:WTG-Dev/tbwa-creative-website.git"
  },
  "bugs": {
    "url": "https://gitlab-eu.factory.tools/WTG-Dev/tbwa-creative-website/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "commit": "git-cz",
    "build:railsdev": "ng build --prod --configuration=rails-dev --build-optimizer --base-href=./ --deploy-url=LibraryWebUI/ ",
    "build:prod": "ng build --prod --configuration=aws-prod --build-optimizer --base-href=/nuxeo/site/LibraryWebUI/ --deploy-url=/nuxeo/LibraryWebUI/ ",
    "build:stg": "ng build --prod --configuration=aws-stg --build-optimizer --base-href=/nuxeo/site/LibraryWebUI/ --deploy-url=/nuxeo/LibraryWebUI/ ",
    "build:dev": "ng build --prod --configuration=aws-dev --build-optimizer --base-href=/nuxeo/site/LibraryWebUI/ --deploy-url=/nuxeo/LibraryWebUI/ ",
    "lint": "ng lint",
    "lint:fix": "ng lint tbwa-creative-website --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ng-bootstrap/ng-bootstrap": "4.1.3",
    "@ng-select/ng-select": "^2.20.5",
    "@nicky-lenaers/ngx-scroll-to": "^2.0.0",
    "angular2-text-mask": "^9.0.0",
    "angular2-toaster": "^7.0.0",
    "bootstrap": "4.1.3",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.5",
    "hammerjs": "^2.0.8",
    "intersection-observer": "0.7.0",
    "ng-click-outside": "^4.0.0",
    "ng2-completer": "3.0.2",
    "ngx-cookie-service": "^2.2.0",
    "ngx-device-detector": "1.3.6",
    "ngx-drag-scroll": "^7.4.3",
    "ngx-pipes": "^2.5.1",
    "ngx-scrollbar": "^4.2.0",
    "pace-js": "1.0.2",
    "parse5": "5.1.0",
    "random-js": "^1.0.8",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "typeface-exo": "0.0.61",
    "videogular2": "^6.4.0",
    "web-animations-js": "2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
