{
  "name": "tbwa-creative-website",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+git@gitlab-ssh-eu.factory.tools:WTG-Dev/tbwa-creative-website.git"
  },
  "bugs": {
    "url": "https://gitlab-eu.factory.tools/WTG-Dev/tbwa-creative-website/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "commit": "git-cz",
    "start": "ng serve",
    "build": "ng build",
    "build:railsdev": "ng build --prod --configuration=rails-dev --build-optimizer --base-href=./ --deploy-url=creative_assets/ ",
    "build:nadev": "ng build --prod --configuration=na-dev --build-optimizer --base-href=./ --deploy-url=/nuxeo/LibraryWebUI/ ",
    "build:awsdev": "ng build --prod --configuration=aws-dev --build-optimizer --base-href=./ --deploy-url=/nuxeo/LibraryWebUI/ ",
    "test": "ng test",
    "test:coverage": "rimraf coverage && yarn test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint && yarn lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.2.6",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.6",
    "@angular/http": "^7.2.6",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@ng-select/ng-select": "^2.15.3",
    "@ngx-progressbar/core": "^5.3.2",
    "@ngx-progressbar/http": "^5.3.2",
    "@ngx-progressbar/router": "^5.3.2",
    "angular2-toaster": "^7.0.0",
    "angulartics2": "^7.4.1",
    "bootstrap": "4.1.3",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.4",
    "eva-icons": "^1.1.1",
    "hammerjs": "^2.0.8",
    "intersection-observer": "0.5.1",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "ng-click-outside": "^4.0.0",
    "ng2-completer": "2.0.8",
    "ngx-cookie-service": "^2.1.0",
    "ngx-device-detector": "1.3.5",
    "ngx-drag-scroll": "^7.1.0",
    "ngx-image-viewer": "^1.0.13",
    "ngx-pipes": "^2.3.10",
    "ngx-scrollbar": "^4.1.1",
    "normalize.css": "8.0.1",
    "pace-js": "1.0.2",
    "parse5": "5.1.0",
    "random-js": "^1.0.8",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.4.0",
    "rxjs-compat": "6.4.0",
    "screenfull": "^4.0.0",
    "simple-pdf-viewer": "^2.0.3",
    "typeface-exo": "0.0.61",
    "typescript": "3.2.4",
    "videogular2": "^6.4.0",
    "web-animations-js": "2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.3",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "7.2.6",
    "@compodoc/compodoc": "1.1.7",
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@types/jasmine": "3.3.5",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "standard-version": "^4.4.0",
    "stylelint": "9.9.0",
    "ts-md5": "^1.2.4",
    "ts-node": "7.0.1",
    "tslint": "5.13.0",
    "tslint-language-service": "^0.9.9"
  }
}
