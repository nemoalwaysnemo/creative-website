    "@angular/language-service": "8.2.14",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "3.3.5",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.6.2",
    "codelyzer": "^5.2.2",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "2.0.27",
