{
  "name": "tbwa-knowledge-website",
  "version": "4.1.1",
  "scripts": {
    "commit": "git-cz",
    "build:replace": "bash ./build.sh",
    "build:stg": "yarn clean && ng build --c=aws-stg && yarn build:replace",
    "build:dev": "yarn clean && ng build --c=aws-dev && yarn build:replace",
    "build:test": "yarn clean && ng build --c=local-dev",
    "build:serve": "yarn clean && ng build --c=local-dev && yarn serve",
    "lint:fix": "ng lint tbwa-knowledge-website --fix",
    "lint:styles": "stylelint ./project/src/**/*.scss --fix",
    "lint:ci": "yarn lint:fix && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version",
    "postinstall": "ngcc",
    "clean": "rimraf project/dist",
    "serve": "lite-server --baseDir=./project/dist/LibraryWebUI"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~11.2.4",
    "@angular/cdk": "~11.2.3",
    "@angular/common": "~11.2.4",
    "@angular/compiler": "~11.2.4",
    "@angular/core": "~11.2.4",
    "@angular/forms": "~11.2.4",
    "@angular/material": "^11.2.3",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "@ng-bootstrap/ng-bootstrap": "5.3.1",
    "@ng-select/ng-select": "^6.1.0",
    "@videogular/ngx-videogular": "^3.0.1",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^4.5.3",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.9.1",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.12.0",
    "ng-click-outside": "^7.0.1",
    "ng2-completer": "^9.0.1",
    "ngx-clipboard": "^14.0.1",
    "ngx-device-detector": "^2.0.6",
    "ngx-drag-scroll": "^11.0.0-beta.1",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-permissions": "^8.1.1",
    "ngx-pipes": "^2.7.5",
    "ngx-scrollbar": "^7.5.4",
    "pace-js": "^1.2.4",
    "parse5": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.6",
    "ts-md5": "^1.2.7",
    "tslib": "^2.1.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "^11.2.3",
    "@angular/compiler-cli": "~11.2.4",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/node": "^14.14.32",
    "codelyzer": "^6.0.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.8",
    "postinstall": "^0.7.0",
    "stylelint": "^13.12.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
