{
  "name": "tbwa-knowledge-website",
  "version": "4.5.0",
  "scripts": {
    "commit": "git-cz",
    "build:replace": "bash ./build.sh",
    "build:stg": "yarn clean && ng build --c=aws-stg && yarn build:replace",
    "build:dev": "yarn clean && ng build --c=aws-dev && yarn build:replace",
    "build:test": "yarn clean && ng build --c=local-dev",
    "build:serve": "yarn clean && ng build --c=local-dev && yarn serve",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint ./project/src/**/*.scss --fix",
    "lint:ci": "yarn lint:fix && yarn lint:styles",
    "release:changelog": "yarn conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:version": "standard-version",
    "postinstall": "ngcc",
    "clean": "rimraf project/dist",
    "serve": "lite-server --baseDir=./project/dist/LibraryWebUI",
    "lint": "ng lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ng-bootstrap/ng-bootstrap": "5.3.1",
    "@ng-select/ng-select": "^7.2.0",
    "@videogular/ngx-videogular": "^4.1.0",
    "angular2-text-mask": "^9.0.0",
    "browserify-zlib": "^0.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.16.1",
    "dexie": "^3.0.3",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.12.0",
    "ng-click-outside": "^9.0.0",
    "ng2-completer": "^9.0.1",
    "ngx-clipboard": "^14.0.1",
    "ngx-device-detector": "^2.1.1",
    "ngx-drag-scroll": "^12.0.0-beta.1",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-permissions": "^8.1.1",
    "ngx-pipes": "^2.7.5",
    "ngx-scrollbar": "^7.6.1",
    "pace-js": "^1.2.4",
    "parse5": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "shufflejs": "^5.4.1",
    "ts-md5": "^1.2.9",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^4.3.8",
    "postinstall": "^0.7.0",
    "stylelint": "^13.13.1",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  }
}
